{
    "name": "kiwfy/time-chamber-test",
    "description": "The Hyperbolic Time Chamber (精神と時の部屋 Seishin to Toki no Heya, literal meaning 'Spirit and Time Room')",
    "version": "1.0.0",
    "type": "library",
    "license": "GPL-3.0-only",
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21"
    },
    "scripts": {
        "test": "phpdbg -qrr vendor/bin/phpunit --configuration phpunit.xml -d memory_limit=1024M"
    },
    "autoload": {
        "psr-4": {
            "Tests1Doc\\": "src/"
        }
    },
    "require": {
        "nesbot/carbon": "^2.57",
        "predis/predis": "^1.1",
        "kiwfy/ulid-php": "^2.0",
        "illuminate/container": "^9.9",
        "illuminate/database": "^9.10"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
